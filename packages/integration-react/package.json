{
  "name": "@copperui/react",
  "sideEffects": false,
  "version": "0.1.1",
  "license": "MIT",
  "description": "Integração do copperui com o React.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rm -rf dist",
    "compile": "yarn run tsc",
    "tsc": "tsc -p ."
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/node": "10.12.9",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@copperui/core": "workspace:^"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/copperui/copperui.git"
  },
  "keywords": [
    "react",
    "copperui"
  ],
  "bugs": {
    "url": "https://github.com/copperui/copperui/issues"
  },
  "homepage": "https://github.com/copperui/copperui#readme",
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
